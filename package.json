{
    "name": "interactive-typography-with-particle-flow-fields",
    "version": "0.0.0-development",
    "description": "Interactive typography experiment with procedural flow fields and particles. Inspired by an original AS3 implementation, reimagined in JavaScript.",
    "scripts": {
        "start": "serve .",
        "build:demos": "rollup -c rollup.demos.config.js",
        "copy:licenses": "rollup -c rollup.copy-licenses.config.js",
        "build": "npm run build:demos && npm run copy:licenses",
        "serve:public": "serve public"
    },
    "keywords": [
        "javascript",
        "canvas",
        "typography",
        "particles",
        "flow fields",
        "interactive",
        "procedural",
        "noise"
    ],
    "author": "github.com/toasted323",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-commonjs": "^28.0.3",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "eslint": "^8.0.0",
        "rollup": "^2.79.2",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-css-only": "^4.5.2",
        "rollup-plugin-svelte": "^7.2.2",
        "serve": "^14.2.0",
        "svelte": "^4.2.20"
    },
    "dependencies": {
        "@material/material-color-utilities": "^0.3.0",
        "svelte-tweakpane-ui": "^1.5.8",
        "tweakpane": "^4.0.5"
    },
    "private": true
}